		"postcss": "^8.4.33",
		"postcss-nesting": "^12.0.2",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.1.6",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module"
}
